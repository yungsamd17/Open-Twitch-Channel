{
   "manifest_version": 2,
   "name": "Twitch URL Replacer",
   "version": "1.0",
   "description": "Replace username in Twitch URL with one of your choice",
   "icons": {
       "16": "icon16.png",
       "48": "icon48.png",
       "128": "icon128.png"
   },
   "browser_action": {
       "default_icon": "icon16.png",
       "default_popup": "popup.html",
       "default_title": "Twitch URL Replacer"
   },
   "permissions": [
       "activeTab"
   ]
}
